<div id="modal-add-edit" class="bootbox modal fade modal-darkorange in" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog  modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="bootbox-close-button close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title">Thông tin kho chứa</h4>
            </div>
            <div class="modal-body">
                <div id="horizontal-form">
                    <form class="form-horizontal" role="form" id="frmMaintainance">
                        <div class="form-group row">
                            <label for="txtId" class="col-sm-2 col-form-label">Mã kho:<span class="star"> *</span></label>
                            <div class="col-sm-2">
                                <input type="text" class="form-control" id="txtId" name="txtId" maxlength="15">
                            </div>
                            <label for="txtWarehouseName" class="col-sm-2 col-form-label">Tên kho chứa: <span class="star">*</span></label>
                            <div class="col-sm-6">
                                <input type="text" class="form-control" id="txtWarehouseName" name="txtWarehouseName" maxlength="50">
                            </div>
                        </div>
                        <div class="form-group row">
                            <label for="txtWarehouseAddress" class="col-sm-2 col-form-label">Địa chỉ: <span class="star">*</span></label>
                            <div class="col-sm-10">
                                <input type="text" class="form-control" id="txtWarehouseAddress" name="txtWarehouseAddress" maxlength="100"/>
                            </div>
                        </div>

                        <div class="form-group row">
                            <div class="col-sm-offset-8 checkbox">
                                <label><input type="checkbox" value="" id="chkStatus">Còn dùng?</label>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-sm-offset-2 col-sm-10">
                                <button type="button" id="btnSave" class="btn btn-success">Ghi</button>
                                <button type="button" id="btnCancel" data-dismiss="modal" class="btn btn-danger">Hủy</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>